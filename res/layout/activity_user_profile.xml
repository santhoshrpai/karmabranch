<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background2"
    android:columnCount="2"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.karmabranch.UserProfile" >

    <Space
        android:id="@+id/Space1"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/ratingBar1" />

    <Space
        android:id="@+id/Space2"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space1" />

    <Space
        android:id="@+id/Space3"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space2" />

    <Space
        android:id="@+id/Space4"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space3" />

    <Space
        android:id="@+id/Space5"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space4" />

    <Space
        android:id="@+id/Space6"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space5" />

    <Space
        android:id="@+id/Space7"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space6" />

    <Space
        android:id="@+id/Space8"
        android:layout_width="1dp"
        android:layout_height="15dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_below="@+id/Space7" />

    <Button
        android:id="@+id/Button01"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Request" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/Button01"
        android:text="Accept" />

    <RatingBar
        android:id="@+id/ratingBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView1"
        android:layout_marginRight="15dp"
        android:rating="3" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button01"
        android:layout_below="@+id/Button01"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="72dp"
        android:text="Username"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="74dp"
        android:layout_height="81dp"
        android:layout_alignBottom="@+id/textView1"
        android:layout_toLeftOf="@+id/textView1"
        android:src="@drawable/ic_launcher" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/Space1"
        android:layout_marginRight="16dp"
        android:text="Read More" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Space1"
        android:layout_alignTop="@+id/Space1"
        android:text="History"
        android:textColor="#000000" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Space8"
        android:layout_alignLeft="@+id/button2"
        android:text="Read More" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ratingBar1"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button4"
        android:text="View Requests" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/ratingBar1"
        android:layout_alignTop="@+id/imageView1"
        android:text="Edit" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/Space3"
        android:text="Leant lawn mover"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignTop="@+id/Space6"
        android:text="Edited term paper"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/ratingBar1"
        android:layout_alignTop="@+id/Space8"
        android:text="Read More" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button4"
        android:layout_alignBottom="@+id/button4"
        android:layout_alignLeft="@+id/textView4"
        android:text="Doctor&apos;s appointment"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000" />

</RelativeLayout>